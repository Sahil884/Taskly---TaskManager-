{
  "installCommand": "npm install --legacy-peer-deps",
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api(.*)", "dest": "src/index.js", "headers": { "Access-Control-Allow-Origin": "https://taskly-task-manager-icrv.vercel.app", "Access-Control-Allow-Credentials": "true" } },
    
  ]
  
}
