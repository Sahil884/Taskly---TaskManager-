{
  "installCommand": "npm install --legacy-peer-deps",
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/", "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"], "headers": { "Access-Control-Allow-Origin": "https://taskly-task-manager-icrv.vercel.app"} }
  ]
}
